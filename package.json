{
	"name": "MDTools",
	"version": "0.0.1",
	"private": false,
	"description": "Text replacement functions e.g. change case, reverse and ASCII Art for all file types.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandMDTools#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandMDTools/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandMDTools.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.textFunctions",
				"description": "Text functions on selections",
				"title": "Text Functions"
			}
		],
		"keybindings": [
			{
				"command": "extension.textFunctions",
				"key": "Alt+T"
			}
		]
	},
	"activationEvents": [
		"onCommand:extension.textFunctions"
	],
	"dependencies": {
		"figlet": "1.7.0",
		"underscore.string": "3.3.6"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/MDToolsIcon.svg",
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	}
}
